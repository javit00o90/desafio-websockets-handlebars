<h1>Products</h1> <a href="/realtimeproducts"><h3>RealTimeProducts</h3></a>

<div id="productosContainerHome">
  {{#each products}}
  <div class="productCard">
      <h2>{{this.title}}</h2>
      <p>Price: ${{this.price}}</p> 
      <p>Stock: {{this.stock}}</p>
      <p>{{this.description}}</p>
  </div>
  {{/each}}
</div>
